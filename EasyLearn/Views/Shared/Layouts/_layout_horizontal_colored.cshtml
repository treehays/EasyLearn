<!DOCTYPE html>
<html lang="en">

<head>

    <partial name="~/Views/Shared/_title_meta.cshtml"/>
    <partial name="~/Views/Shared/_head_css.cshtml"/>

</head>

<body data-topbar="colored" data-layout="horizontal">

    <!-- Begin page -->
    <div id="layout-wrapper">

        <partial name="~/Views/Shared/_topbar.cshtml"/>
        <partial name="~/Views/Shared/_horizontal.cshtml"/>

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->

        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">

                    <partial name="~/Views/Shared/_page_title.cshtml"/>

                    @RenderBody()

                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

            <partial name="~/Views/Shared/_footer.cshtml"/>

        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->

    <partial name="~/Views/Shared/_right_sidebar.cshtml"/>

    <!-- Right bar overlay-->
    <div class="rightbar-overlay"></div>

    @*<partial name="~/Views/_Shared/_vendor_scripts.cshtml"/>*@

    @RenderSection("scripts", required: false)


</body>

</html>
